{
  "private": true,
  "name": "typescript-pgtyped-starter",
  "version": "0.0.5-alpha",
  "scripts": {
    "env": "unset NODE_ENV && export $(grep '^#' .env -v | xargs)",
    "env:print": "npm run env && printenv",
    "webpack:build": "unset NODE_ENV && dotenv -e .env webpack --config tools/webpack/webpack.config.js",
    "webpack:start": "webpack serve --config tools/webpack/webpack.config.js",
    "postgres:reset": "npm run -s env | docker-compose --env-file .env --file ./server/docker/compose-postgres.yml down --volumes",
    "postgres:compose": "npm run -s postgres:reset && docker-compose --env-file .env --file ./server/docker/compose-postgres.yml up --no-start",
    "postgres:start": "docker-compose --env-file .env --file ./server/docker/compose-postgres.yml start",
    "postgres:stop": "docker-compose --env-file .env --file ./server/docker/compose-postgres.yml stop",
    "postgres:logs": "export $(cat .env | xargs) && env-cmd --file ./.env docker logs --tail 50 --follow --timestamps $POSTGRES_DOCKER_CONTAINER",
    "postgres:bash": "export $(cat .env | xargs) && env-cmd --file ./.env docker exec -it $POSTGRES_DOCKER_CONTAINER bash",
    "postgres:extensions": "env-cmd --file .env tsx ./src/postgres/showActiveExtensions.ts",
    "postgrest:reset": "npm run -s env | docker-compose --env-file .env --file ./server/docker/compose-postgrest.yml down --volumes",
    "postgrest:compose": "npm run -s postgrest:reset && docker-compose --env-file .env --file ./server/docker/compose-postgrest.yml up --no-start",
    "postgrest:start": "docker-compose --env-file .env --file ./server/docker/compose-postgrest.yml start",
    "postgrest:stop": "docker-compose --env-file .env --file ./server/docker/compose-postgrest.yml stop",
    "postgrest:logs": "export $(cat .env | xargs) && env-cmd --file ./.env docker logs --tail 50 --follow --timestamps $POSTGREST_DOCKER_CONTAINER",
    "swagger:reset": "npm run -s env | docker-compose --env-file .env --file ./server/docker/compose-swagger.yml down --volumes",
    "swagger:compose": "npm run -s swagger:reset && docker-compose --env-file .env --file ./server/docker/compose-swagger.yml up --no-start",
    "swagger:start": "docker-compose --env-file .env --file ./server/docker/compose-swagger.yml start",
    "swagger:stop": "docker-compose --env-file .env --file ./server/docker/compose-swagger.yml stop",
    "swagger:logs": "export $(cat .env | xargs) && env-cmd --file ./.env docker logs --tail 50 --follow --timestamps $SWAGGER_DOCKER_CONTAINER",
    "mongo:reset": "npm run -s env | docker-compose --env-file .env --file ./server/docker/compose-mongo.yml down --volumes",
    "mongo:compose": "npm run -s mongo:reset && docker-compose --env-file .env --file ./server/docker/compose-mongo.yml up --no-start",
    "mongo:start": "docker-compose --env-file .env --file ./server/docker/compose-mongo.yml start",
    "mongo:stop": "docker-compose --env-file .env --file ./server/docker/compose-mongo.yml stop",
    "mongo:shell": "docker exec -it mongo_container mongosh",
    "mongo:logs": "export $(cat .env | xargs) && env-cmd --file ./.env docker logs --tail 50 --follow --timestamps $MONGO_DOCKER_CONTAINER",
    "pgtyped:start": "npx pgtyped -w -c ./pgtyped-config.json",
    "pgtyped:all": "npm run -s postgres:reset && npm run -s postgres:compose && npm run -s postgres:start && npm run pgtyped:watch",
    "api:start": "npx ts-node --project server/api/tsconfig.json server/api/server.ts",
    "prettier": "prettier --write \"./**/*.{js,ts,json}\"",
    "docker:show": "echo '\n\n███ CONTAINERS\n' && docker ps && echo '\n███ VOLUMES\n' && docker volume ls && echo '\n███ IMAGES\n' && docker image ls && echo '\n███ NETWORKS\n' && docker network ls && echo '\n\n' ",
    "docker:containers:stop": "docker ps -aq | xargs -r docker stop",
    "docker:containers:remove": "docker ps -aq | xargs -r docker rm",
    "docker:network:create": "export $(cat .env | xargs) && env-cmd --file ./.env docker network create -d bridge ${DOCKER_NETWORK}",
    "docker:network:remove": "export $(cat .env | xargs) && env-cmd --file ./.env docker network rm ${DOCKER_NETWORK}",
    "deps:check": "npm-check --skip-unused",
    "deps:update": "npm-check --update --skip-unused --save-exact"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dietrich-stein/typescript-pgtyped-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "homepage": "",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.12.18",
    "@pgtyped/cli": "^2.3.0",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/node": "^20.6.3",
    "@types/pg": "^8.10.5",
    "@types/webpack": "^5.28.1",
    "@types/webpack-dev-server": "^4.7.2",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.9.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-check": "^6.0.1",
    "postcss": "8.3",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.7",
    "require-json5": "^1.1.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@pgtyped/runtime": "^2.3.0",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "typescript-rest": "^3.0.4"
  },
  "engines": {
    "npm": ">=9.8.1 <10.0.0",
    "node": ">=18.18.2 <19.0.0"
  }
}
