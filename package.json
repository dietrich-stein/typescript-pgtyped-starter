{
  "private": true,
  "name": "typescript-pgtyped-rest-starter",
  "version": "1.0.0",
  "scripts": {
    "webpack:build": "unset NODE_ENV && dotenv -e .env webpack --config tools/webpack/webpack.config.js",
    "webpack:build:production": "unset NODE_ENV && dotenv -e .env.production webpack --config tools/webpack/webpack.config.js",
    "webpack:start": "webpack serve --config tools/webpack/webpack.config.js",
    "db:reset": "docker-compose down --file ./server/database/compose.yml --volumes && docker volume rm -f typescript-starter_db",
    "db:compose": "npm run db:reset && docker-compose --file ./server/database/compose.yml --env-file ./env up --no-start",
    "db:compose:production": "npm run db:reset && docker-compose --file ./server/database/compose.yml --env-file ./env.production up --no-start",
    "db:start": "docker-compose start --file ./server/database/compose.yml --env-file ./env",
    "db:start:production": "docker-compose start --file ./server/database/compose.yml --env-file ./env.production",
    "pgtyped:start": "npx pgtyped -w -c ./pgtyped-config.json",
    "pgtyped:all": "npm run db:reset && npm run db:compose && npm run db:start && npm run pgtyped:watch",
    "api:start": "npx ts-node --project server/api/tsconfig.json server/api/server.ts",
    "utils:prettier": "prettier --write \"./**/*.{js,ts,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dietrich-stein/typescript-pgtyped-rest-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "homepage": "",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.12.18",
    "@pgtyped/cli": "^2.3.0",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/node": "^20.6.3",
    "@types/pg": "^8.10.5",
    "@types/webpack": "^5.28.1",
    "@types/webpack-dev-server": "^4.7.2",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.9.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "8.3",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.7",
    "require-json5": "^1.1.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@pgtyped/runtime": "^2.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "typescript-rest": "^3.0.4"
  }
}
